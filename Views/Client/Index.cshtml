@model IEnumerable<ClientModel>
@{
    ViewData["Title"]="Index";
}
<h1>Клиенты</h1>
<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                Номер
            </th>
            <th>
                Название
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td width="50%">
                    @obj.Id
                </td>
                <td width="50%">
                    @obj.Name
                </td>
            </tr>
        }
    </tbody>
</table>
